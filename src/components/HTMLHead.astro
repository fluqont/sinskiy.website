---
	const title = "Sinskiy";
	const description = "My personal website with projects, information about myself and contact information";
---
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" type="image/svg+xml" href="logo.svg" />
    <meta name="generator" content={Astro.generator} />
	<link rel="preconnect" href="https://fonts.bunny.net">
	<link href="https://fonts.bunny.net/css?family=red-rose:300,400,700" rel="stylesheet" />

    <!-- General Meta Tags -->
    <title>{title}</title>
    <meta name="title" content={title} />
    <meta name="description" content={description} />
    <meta name="author" content="Sinskiy" />
</head>